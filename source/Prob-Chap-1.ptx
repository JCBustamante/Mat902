    <figure>
      <image>
        <latex-image>
        \begin{tikzpicture}
         \coordinate (A) at (0,0);
         \coordinate (B) at (-2,2);
         \coordinate (C) at (2,4);
         \coordinate (D) at (8,2);
         \coordinate (P) at (-1,1);
         \coordinate (Q) at (0,3);
         \coordinate (R) at (5,3);
         \coordinate (S) at (4,1);
         
 
         \draw [myblue, thick] (A)--(B)--(C)--(D)--(A);
         \draw [myred, thick] (P)--(Q)--(R)--(S)--(P);
         \tkzMarkSegment[mark=s||, myblue](A,P);
         \tkzMarkSegment[mark=s||, myblue](P,B);
         \tkzMarkSegment[mark=s|, myblue](B,Q);
         \tkzMarkSegment[mark=s|, myblue](Q,C);
         \tkzMarkSegment[mark=s|||, myblue](C,R);
         \tkzMarkSegment[mark=s|||, myblue](R,D);
         \tkzMarkSegment[mark=|||, myblue](D,S);
         \tkzMarkSegment[mark=|||, myblue](A,S);
         
 
         \filldraw [myblue] (A) circle [radius=2pt] node[left] {$A$}
                  (C) circle [radius=2pt] node[above] {$C$}
                  (B) circle [radius=2pt] node[left] {$B$}
                  (D) circle [radius=2pt] node[right] {$D$};
        \filldraw [myred]
                  (P) circle [radius=2pt] node[below left] {$P$}
                  (Q) circle [radius=2pt] node[above left] {$Q$}
                  (S) circle [radius=2pt] node[below right] {$S$}
                  (R) circle [radius=2pt] node[above right] {$R$};
        
        \end{tikzpicture}
        </latex-image>
      </image>  
    </figure>
    </sidebyside>
  </statement>
  
  
</exercise>


